<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light.NoTitleBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="Common" />
    <style name="Common.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:padding">25dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#766c63</item>
        <item name="android:background">@drawable/btn_common</item>
    </style>
    <style name="Common.Hea">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">#717373</item>
    </style>
    <style name="Common.Hea.der">
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="Common.TitleText">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    
    <style name="Main" />
    <style name="Main.MessageText">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    <style name="Main.ButtonText">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    <style name="Main.ButtonText.Small">
        <item name="android:layout_height">75dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>
    <style name="Main.ButtonHeightBig">
        <item name="android:layout_height">160dp</item>
    </style>
    <style name="Main.ButtonText2">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#a29487</item>
    </style>
    <style name="Main.ButtonText3">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#2d2925</item>
    </style>
    
    <style name="Game" />
    <style name="Game.ButtonText">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    <style name="Game.SubButtonText">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#763c33</item>
    </style>
    <style name="Game.TableTitle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#eee4da</item>
    </style>
    <style name="Game.TableNumberBig">
        <item name="android:textSize">27sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Game.TableNumberSmall">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:drawablePadding">4dp</item>
    </style>
    <style name="Game.TableNumberSmall.Mid">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="Game.TitleMissionText">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="Game.RecommendText">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="Game.MissionLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    
    <style name="Settings" />
    <style name="Settings.ButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:paddingTop">18dp</item>
        <item name="android:paddingBottom">18dp</item>
        <item name="android:background">@drawable/btn_common</item>
    </style>
    <style name="Settings.ButtonText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    <style name="Settings.ButtonState">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="Settings.ButtonState.On">
        <item name="android:text">ON</item>
        <item name="android:textColor">#22ac38</item>
    </style>
    <style name="Settings.ButtonState.Off">
        <item name="android:text">OFF</item>
        <item name="android:textColor">#e60012</item>
    </style>
    <style name="Settings.BuyItemText">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    
    <style name="Share" />
    <style name="Share.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/btn_common</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#766c63</item>
    </style>
    
    <style name="Game2" />
    <style name="Game2.ProfileName">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    	<item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="Game2.RemainTime">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    	<item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    
    <style name="Info" />
    <style name="Info.Title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
    	<item name="android:textSize">20sp</item>
        <item name="android:textColor">#c7514b</item>
    </style>
    <style name="Info.Text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    	<item name="android:textSize">18sp</item>
        <item name="android:textColor">#37312b</item>
    </style>
    
    <style name="Ufo" />
    <style name="Ufo.NoticeTitle">
    	<item name="android:textSize">25sp</item>
        <item name="android:textColor">#ffa0a0</item>
    </style>
    <style name="Ufo.NoticeMessage">
        <item name="android:layout_marginTop">10dp</item>
    	<item name="android:textSize">20sp</item>
        <item name="android:textColor">#766c63</item>
        <item name="android:lineSpacingExtra">5dp</item>
    </style>
    <style name="Ufo.NoticePointTitle">
        <item name="android:layout_marginTop">20dp</item>
    	<item name="android:textSize">15sp</item>
        <item name="android:textColor">#938171</item>
    </style>
    <style name="Ufo.NoticePoint">
    	<item name="android:textSize">30sp</item>
        <item name="android:textColor">#52483e</item>
    </style>
    <style name="Ufo.ScoreBody">
        <item name="android:layout_marginTop">20dp</item>
    </style>
    <style name="Ufo.ScoreLeft">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">right</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#938171</item>
    </style>
    <style name="Ufo.ScoreRight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#52483e</item>
    </style>
    <style name="Ufo.PauseBtn">
    	<item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Ufo.PauseBtn.Text">
    	<item name="android:textSize">25sp</item>
    </style>
    <style name="Ufo.GoBackToCount">
    	<item name="android:textSize">17sp</item>
    </style>
    <style name="Ufo.InviterImg">
    	<item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="Ufo.CommonText">
    	<item name="android:textSize">25sp</item>
        <item name="android:textColor">@drawable/ufo_alert_btn_text_color</item>
    </style>
    <style name="Ufo.ConfirmText">
    	<item name="android:textSize">22sp</item>
    </style>
    <style name="Ufo.AcceptDeclineBtn">
    	<item name="android:textSize">22sp</item>
    </style>
    <style name="Ufo.ExTouchPaneText">
    	<item name="android:textSize">14sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

</resources>
